<?xml version="1.0"?>
<JWM>
 <!-- The root menu, if this is undefined you will not get a menu. -->
 <!-- Additional RootMenu attributes: onroot, labeled, label -->
 <RootMenu height="24" onroot="3">
     <Program icon="navegador.png" label="Navegador">midori</Program>
     <Program icon="explorador.png" label="Gestor de ficheros">pcmanfm</Program>
     <Program icon="diraya.png" label="Diraya">/opt/Citrix/ICAClient/wfica /etc/diraya.ica</Program>
  <Program icon="terminal.png" label="Terminal">rxvt</Program> 
   <Separator/>
   <Exit label="Salir" confirm="false" icon="salir.png"/>

 </RootMenu>

