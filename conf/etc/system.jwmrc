 <!-- Styles -->
 <WindowStyle>
  <Font antialias="true">xft:DejaVuSans-6:bold</Font>
  <Width>6</Width>
  <Height>24</Height>
  <Active>
   <Text>#bababa</Text>
   <Title>#474c52:#303438</Title>
   <Outline>grey</Outline>
  </Active>
  <Inactive>
   <Text>white</Text>
   <Title>#676a6d:#7e8b9b</Title>
   <Outline>grey</Outline>
  </Inactive>
 </WindowStyle>
 <TaskListStyle>
  <Font antialias="true">xft:DejaVuSansLight-6</Font>
  <ActiveForeground>black</ActiveForeground>
  <ActiveBackground>gray:#C1E9EB</ActiveBackground>
  <Foreground>gray</Foreground>
  <Background>#4f546a</Background>
 </TaskListStyle>
 <TrayStyle>
  <Font antialias="true">xft:DejaVuSansLight-6</Font>
  <Background>#474c52</Background>
  <Foreground>white</Foreground>
  <Opacity>0.5</Opacity>
 </TrayStyle>
 <MenuStyle>
  <Font antialias="true">xft:DejaVuSansLight-6</Font>
  <Foreground>#cccccc</Foreground>
  <Background>#474c52</Background>
  <ActiveForeground>black</ActiveForeground>
  <ActiveBackground>gray:#C1E9EB</ActiveBackground>
 </MenuStyle>
 <PopupStyle enabled="true">
  <Font antialias="true">xft:DejaVuSansLight-6</Font>
  <Outline>black</Outline>
  <Foreground>black</Foreground>
  <Background>gray</Background>
 </PopupStyle>
 <IconPath>
 /usr/share/wbar/
 </IconPath>
 <Desktops width="1" height="1">
 </Desktops>
 
 <!-- Double click speed (in milliseconds) -->
 <DoubleClickSpeed>400</DoubleClickSpeed>
 <!-- Double click delta (in pixels) -->
 <DoubleClickDelta>2</DoubleClickDelta>
 <!-- The focus model (sloppy or click) -->
 <FocusModel>click</FocusModel>
 <!-- The snap mode (none, screen, or border) -->
 <SnapMode distance="10">border</SnapMode>
 <!-- The move mode (outline or opaque) -->
 <MoveMode>outline</MoveMode>
 <!-- The resize mode (outline or opaque) -->
 <ResizeMode>outline</ResizeMode>
 <StartupCommand></StartupCommand>
 <!-- Key bindings -->
 <Key key="Up">up</Key>
 <Key key="Down">down</Key>
 <Key key="Right">right</Key>
 <Key key="Left">left</Key>
 <Key key="h">left</Key>
 <Key key="j">down</Key>
 <Key key="k">up</Key>
 <Key key="l">right</Key>
 <Key key="Return">select</Key>
 <Key key="Escape">escape</Key>
 <Key mask="A" key="Tab">nextstacked</Key>
 <Key mask="A" key="F4">close</Key>
 <Key mask="A" key="#">desktop#</Key>
 <Key mask="A" key="F2">window</Key>
 <Key mask="A" key="F10">maximize</Key>
 <Key mask="A" key="Right">rdesktop</Key>
 <Key mask="A" key="Left">ldesktop</Key>
 <Key mask="A" key="Up">udesktop</Key>
 <Key mask="A" key="Down">ddesktop</Key>

<Include>$HOME/.jwmrc-desktop</Include>

</JWM>
