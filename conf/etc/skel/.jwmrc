<?xml version="1.0"?>
<JWM>
   <IconPath>
   /usr/share/icons/letsas
   </IconPath>
   <Group>
      <Name>noticias</Name>
      <Class>noticias</Class>
      <Option>noborder</Option>
      <Option>layer:below</Option>
      <Option>nofocus</Option>
      <Option>nolist</Option>
      <Option>notitle</Option>
      <Option>sticky</Option>
   </Group>
   <Group>
      <Name>widgets</Name>
      <Class>widgets</Class>
      <Option>nomin</Option>
      <Option>sticky</Option>
      <Option>nofocus</Option>
      <Option>nomove</Option>
      <Option>noresize</Option>
      <Option>layer:below</Option>
   </Group>
   <Group>
      <Name>tint2</Name>
      <Class>tint2</Class>
      <Option>nomin</Option>
      <Option>sticky</Option>
      <Option>nofocus</Option>
      <Option>nomove</Option>
      <Option>noresize</Option>
      <Option>layer:below</Option>
   </Group>
   <MenuStyle>
      <Class>4</Class>
      <Font antialias="true">xft:dejavusans-24</Font>
      <Foreground>#ffffff</Foreground>
      <Background>#262a33</Background>
      <Outline>#262a33</Outline>
      <Opacity>1</Opacity>
      <ActiveForeground>#ffffff</ActiveForeground>
      <ActiveBackground>#32c644</ActiveBackground>
   </MenuStyle>
   <MenuStyle>
      <Class>5</Class>
      <Font antialias="true">xft:dejavusans-24</Font>
      <Foreground>#ffffff</Foreground>
      <Background>#262a33</Background>
      <Outline>#262a33</Outline>
      <Opacity>1</Opacity>
      <ActiveForeground>#ffffff</ActiveForeground>
      <ActiveBackground>#df562c</ActiveBackground>
   </MenuStyle>
   <MenuStyle>
      <Class>6</Class>
      <Font antialias="true">xft:dejavusans-24</Font>
      <Foreground>#ffffff</Foreground>
      <Background>#262a33</Background>
      <Outline>#262a33</Outline>
      <Opacity>1</Opacity>
      <ActiveForeground>#ffffff</ActiveForeground>
      <ActiveBackground>#9ed055</ActiveBackground>
   </MenuStyle>
   <MenuStyle>
      <Class>7</Class>
      <Font antialias="true">xft:dejavusans-24</Font>
      <Foreground>#ffffff</Foreground>
      <Background>#262a33</Background>
      <Outline>#262a33</Outline>
      <Opacity>1</Opacity>
      <ActiveForeground>#ffffff</ActiveForeground>
      <ActiveBackground>#d6ba32</ActiveBackground>
   </MenuStyle>
   <MenuStyle>
      <Font antialias="true">xft:dejavusans-24</Font>
      <Foreground>#ffffff</Foreground>
      <Background>#262a33</Background>
      <Outline>#262a33</Outline>
      <Opacity>1</Opacity>
      <ActiveForeground>#ffffff</ActiveForeground>
      <ActiveBackground>#df562c</ActiveBackground>
   </MenuStyle>
   <WindowStyle>
       <Font antialias="true">xft:DejaVuSans-6:bold</Font>
       <Width>6</Width>
      <Height>24</Height>
      <Active>
        <Text>#bababa</Text>
        <Title>#474c52:#303438</Title>
        <Outline>grey</Outline>
      </Active>
      <Inactive>
        <Text>white</Text>
        <Title>#676a6d:#7e8b9b</Title>
        <Outline>grey</Outline>
      </Inactive>
   </WindowStyle>
   <PopupStyle enabled="true">
        <Font antialias="true">xft:DejaVuSansLight-6</Font>
        <Outline>black</Outline>
        <Foreground>black</Foreground>
        <Background>gray</Background>
   </PopupStyle>
   <TaskListStyle>
  	<Font antialias="true">xft:DejaVuSansLight-6</Font>
  	<ActiveForeground>black</ActiveForeground>
  	<ActiveBackground>gray:#C1E9EB</ActiveBackground>
  	<Foreground>gray</Foreground>
  	<Background>#4f546a</Background>
   </TaskListStyle>
   <TrayStyle>
  	<Font antialias="true">xft:DejaVuSansLight-6</Font>
  	<Background>#474c52</Background>
  	<Foreground>white</Foreground>
  	<Opacity>0.5</Opacity>
   </TrayStyle>

   <!-- Double click speed (in milliseconds) -->
   <DoubleClickSpeed>400</DoubleClickSpeed>
   <!-- Double click delta (in pixels) -->
   <DoubleClickDelta>2</DoubleClickDelta>
   <!-- The focus model (sloppy or click) -->
   <FocusModel>click</FocusModel>
   <!-- The snap mode (none, screen, or border) -->
   <SnapMode distance="10">border</SnapMode>
   <!-- The move mode (outline or opaque) -->
   <MoveMode>outline</MoveMode>
   <!-- The resize mode (outline or opaque) -->
   <ResizeMode>outline</ResizeMode>

